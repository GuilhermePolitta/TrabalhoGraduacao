CREATE TABLE orders (
    customer_id INT,
    amount DOUBLE
);

CREATE TABLE things (
    order_id INT,
    amount DOUBLE,
    FOREIGN KEY (order_id) REFERENCES orders(customer_id)
);

CREATE TABLE customers (
    customer_id INT AUTO_INCREMENT PRIMARY KEY,
    name VARCHAR(255),
    cel_number INT,
	order_id INT,
	amount DOUBLE,
    FOREIGN KEY (order_id) REFERENCES orders(bla)
);
